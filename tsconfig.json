{
  "extends": "./kausal_common/configs/tsconfig.json",
  "compilerOptions": {
    "jsx": "react-jsx",
    "paths": {
      "@/*": ["./src/*"],
      "@common/*": ["./kausal_common/src/*"],
      "/*": ["./src/*"]
    }
  },
  "include": [
    "next-env.d.ts",
    "src/**/*",
    "src/*",
    "*.config.mjs",
    "*.config.ts",
    "*.config.js",
    "codegen.ts"
  ],
  "exclude": ["node_modules"]
}
